<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity2">

    <!--<Button-->
            <!--android:text="Button"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:id="@+id/button5"-->
            <!--android:onClick="trangingOfDay"-->
    <!--/>-->

    <TableLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tableLayout"
    ></TableLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp"
            android:gravity="right">
        <Button
                android:text="UpStairs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/upStairs"
                android:onClick="startUpStairsActivity"

                />
        <Button
                android:text="Circle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Circle"
                android:onClick="startCircleActivity"
                android:nextFocusDown="@id/upStairs"/>
        <Button
                android:text="Exercises"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/buttonExercises"
                android:onClick="startExercises"
        />
        <Button
                android:text="Motivation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/button7"/>
        <Button
                android:text="Date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/button8"
                android:onClick="dataTime"
        />
        <Button
                android:text="Make foto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/button9"
                android:onClick="makeFoto"
        />

    </LinearLayout>


</android.support.constraint.ConstraintLayout>